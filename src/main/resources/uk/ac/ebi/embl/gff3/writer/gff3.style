--[[
-- This is the style sheet for AnnotationSketch.

-- The following SQL produces a default style for each feature.

select 
  'ENA_' || translate(fkey, '''-', chr(95) || chr(95)) ||
  ' = {' || chr(10) ||
  '  stroke             = {red=0.0, green=0.0, blue=0.0, alpha = 1.0},' || chr(10) ||
  '  stroke_marked      = {red=1.0, green=0.0, blue=0.0},' || chr(10) ||
  '  fill               = {red=0.9, green=0.9, blue=1.0},' || chr(10) ||
  '  style              = "box",' || chr(10) ||
  '  collapse_to_parent = false,' || chr(10) ||
  '  split_lines        = true,' || chr(10) ||
  '  max_capt_show_width= nil,' || chr(10) ||
  '  max_show_width     = nil,' || chr(10) ||
  '},' || chr(10) ||
  'ENA_segment_' || translate(fkey, '''-', chr(95) || chr(95)) ||
  ' = {' || chr(10) ||
  '  stroke             = {red=0.0, green=0.0, blue=0.0, alpha = 1.0},' || chr(10) ||
  '  stroke_marked      = {red=1.0, green=0.0, blue=0.0},' || chr(10) ||
  '  fill               = {red=0.0, green=0.0, blue=0.5},' || chr(10) ||
  '  style              = "box",' || chr(10) ||
  '  collapse_to_parent = true,' || chr(10) ||
  '  split_lines        = true,' || chr(10) ||
  '  max_capt_show_width= nil,' || chr(10) ||
  '  max_show_width     = nil,' || chr(10) ||
  '},' || chr(10)
from cv_fkey;

-- There is one parent and one segment style for each feature. Encapsulation
-- of segments into parents is required for visualising segmented features.

  <parent> = {
    stroke             = {red=0.0, green=0.0, blue=0.0, alpha = 1.0},
    stroke_marked      = {red=1.0, green=0.0, blue=0.0},
    fill               = {red=0.9, green=0.9, blue=1.0},
    style              = "box",
    collapse_to_parent = false,
    split_lines        = true,
    max_capt_show_width= nil,
    max_show_width     = nil,
  },  
  <child> = {
    stroke             = {red=0.0, green=0.0, blue=0.0, alpha = 1.0},
    stroke_marked      = {red=1.0, green=0.0, blue=0.0},
    fill               = {red=0.9, green=0.9, blue=0.2},
    style              = "box",
    collapse_to_parent = true,
    split_lines        = true,
    max_capt_show_width= nil,
    max_show_width     = nil,
  },
]]--

